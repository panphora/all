{
  "name": "alldom",
  "version": "1.5.0",
  "description": "A modern DOM manipulation library that selects all elements naturally with native JavaScript feel",
  "homepage": "https://github.com/panphora/all",
  "type": "module",
  "main": "dist/all.umd.js",
  "module": "dist/all.esm.js",
  "unpkg": "dist/all.umd.min.js",
  "jsdelivr": "dist/all.umd.min.js",
  "exports": {
    ".": {
      "import": "./dist/all.esm.js",
      "require": "./dist/all.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js --watch",
    "prepublishOnly": "npm run build",
    "pub": "npm publish --access public"
  },
  "keywords": [
    "jquery",
    "dom",
    "manipulation",
    "selector",
    "javascript"
  ],
  "author": "David Miranda",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.24.0"
  }
}
